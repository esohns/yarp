/* Define to the address where bug reports for this package should be sent. */
#cmakedefine yarp_PACKAGE_BUGREPORT "@PACKAGE_BUGREPORT@"

/* Define to the full name of this package. */
#cmakedefine yarp_PACKAGE_NAME "@PACKAGE_NAME@"

/* Define to the full name and version of this package. */
#cmakedefine yarp_PACKAGE_STRING "@PACKAGE_STRING@"

/* Define to the one symbol short name of this package. */
#cmakedefine yarp_PACKAGE_TARNAME "@PACKAGE_TARNAME@"

/* Define to the home page for this package. */
#cmakedefine yarp_PACKAGE_URL "@PACKAGE_URL@"

/* Define to the version of this package. */
#cmakedefine yarp_PACKAGE_VERSION "@PACKAGE_VERSION@"
#cmakedefine yarp_PACKAGE_VERSION_FULL "@PACKAGE_VERSION_FULL@"

#cmakedefine yarp_PACKAGE_DESCRIPTION "@PACKAGE_DESCRIPTION@"
#cmakedefine yarp_PACKAGE_DESCRIPTION_SUMMARY "@PACKAGE_DESCRIPTION_SUMMARY@"

/* Version numbers of package */
//#cmakedefine Common_VERSION_MAJOR @VERSION_MAJOR@
//#cmakedefine Common_VERSION_MINOR @VERSION_MINOR@
